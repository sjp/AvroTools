@namespace("testing")
protocol Comments {
  /** Documented Enum */
  enum DocumentedEnum {
    /** Dangling Enum3 */ A,
    /** Dangling Enum4 */ B,
    /** Dangling Enum5 */ C
    } = A;

  enum UndocumentedEnum {D,E}

  /** Documented Fixed Type */
  fixed DocumentedFixed(16);

  fixed UndocumentedFixed(16);

  /** Documented Error */
  error DocumentedError {
    string
    /** Documented Reason Field */
    reason,
    /** Default Doc Explanation Field */
    explanation;
  }

  record UndocumentedRecord {
    string description;
  }

  /** Documented Method */
  void documentedMethod(
    /** Documented Parameter */
    string message,
    /** Default Documented Parameter */
