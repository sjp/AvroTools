// Currently genavro only does Protocols.
@namespace("org.apache.avro")
protocol InteropProtocol {
  record Foo {
    string label;
  }

  enum Kind { A, B, C }
  fixed MD5(16);

  record Node {
    string label;
    array<Node> children = [];
  }

  record Interop {
    int intField = 1;
    long longField = -1;
    string stringField;
    boolean boolField = false;
    float floatField = 0.0;
    double doubleField = -1.0e12;
    null nullField;
    array<double> arrayField = [];
    map<Foo> mapField;
    union { boolean, double, array<bytes> } unionFIeld;
    Kind enumField;
    MD5 fixedField;
    Node recordField;
  }

}
